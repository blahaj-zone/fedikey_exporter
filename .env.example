# The base URL of your Misskey/Firefish/Sharkey instance API
API_BASE_URL=http://localhost:3000/api

# Your API token with admin privileges
# Required for accessing admin endpoints like queue stats
API_TOKEN=your_admin_token_here

# Port to run the metrics server on (optional, default: 9125)
PORT=9125

# Interval in milliseconds between metric collections (optional, default: 15000)
SCRAPE_INTERVAL=15000
